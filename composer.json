{
  "name": "mkamelmasoud/starter-core-kit",
  "description": "A reusable Laravel starter kit for custom exception handling, middleware, and shared utilities.",
  "version": "1.2.0",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "package",
    "starter-kit",
    "exception-handler",
    "middleware",
    "utilities",
    "core-kit"
  ],
  "authors": [
    {
      "name": "Mostafa Kamel",
      "email": "mostafakamel000@email.com"
    }
  ],
  "support": {
    "issues": "https://github.com/coder0010/starter-core-kit/issues",
    "source": "https://github.com/coder0010/starter-core-kit"
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "laravel/pint": "^1.24",
    "nunomaduro/larastan": "^3.7",
    "orchestra/testbench": "^8.0 || ^9.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpunit/phpunit": "^10.5"
  },
  "suggest": {
    "tucker-eric/eloquentfilter": "Use for advanced model filtering support"
  },
  "autoload": {
    "psr-4": {
      "MkamelMasoud\\StarterCoreKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MkamelMasoud\\StarterCoreKit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MkamelMasoud\\StarterCoreKit\\PackageServiceProvider"
      ]
    }
  },
  "scripts": {
    "dump": [
      "composer dump-autoload -o"
    ],
    "pint": [
      "./vendor/bin/pint"
    ],
    "phpstan": [
      "@pint",
      "./vendor/bin/phpstan clear-result-cache --configuration=phpstan.neon.dist",
      "./vendor/bin/phpstan --configuration=phpstan.neon.dist --memory-limit=2G"
    ],
    "phpstan-analyse": "./vendor/bin/phpstan analyse -vvv",
    "phpstan-baseline": [
      "./vendor/bin/phpstan analyse --configuration=phpstan.neon.dist --generate-baseline"
    ],
    "setup-backend-local": [
      "composer clear-cache",
      "rm -rf vendor/ composer.lock",
      "composer install --no-interaction",
      "@dump"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
