{
    "name": "mkamelmasoud/starter-core-kit",
    "description": "A reusable Laravel starter kit for custom exception handling, middleware, and shared utilities.",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "package",
        "starter-kit",
        "exception-handler",
        "middleware"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mostafa Kamel",
            "email": "mostafakamel000@email.com"
        }
    ],
    "require": {
        "php": ">=8.0 <8.6",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/framework": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "MkamelMasoud\\StarterCoreKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MkamelMasoud\\StarterCoreKit\\Tests\\": "src/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MkamelMasoud\\StarterCoreKit\\PackageServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    }
}
